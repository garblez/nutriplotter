{
  "expo": {
    "name": "nutriplotterapp",
    "description": "Create healthy, balanced plates of food. Try to get the best score!",
    "slug": "nutriplotterapp",
    "privacy": "public",
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.maxkb.nutriplotter"
    },
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ]
  }
}
